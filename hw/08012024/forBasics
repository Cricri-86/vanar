//for loop 
// standard for formula


//count from 1 to 10

for(let n = 10; n >=1; n--){
    console.log(n);
}




//Album of 9 pictures rewrite with for

let i = 1

for(let i = 1; i <= 9; i++){
    document.write(`<!-- ${i} start -->`);
    document.write(`
    <img src="images/${i}-300x300.jpg">
    `)

    if (i%3 == 0){
        document.write(`<br>`);
    }

    document.write(`<!-- ${i} end -->`);

}
